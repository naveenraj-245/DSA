class Solution {
    public int majorityElement(int[] arr) {
        int count = 0;
        int el = 0;
        for (int i=0; i<arr.length; i++){
            if (count == 0){
                el = arr[i];
            }
            if (el == arr[i]) count++;
            else count--;
        }

        return el;
    }
}